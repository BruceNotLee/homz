    <div class="nav_menu">
      <ul class="right hide-on-med-and-down">
        <li><%= link_to 'My Favorites', '/favorites' %></li>
        <li> <%= link_to 'My Pictures', '/pics' %> </li>
        <li><a href="#">My Messages</a></li>
        <li> <%= link_to "Upload", new_picture_path %> </li>
        <li><%= render 'users/login_items' %></li>
      </ul>
    </div>
  </nav>

<!-- Sidenav Contents -->
<ul class="sidenav" id="mobile-demo">
  <li><%= link_to 'My Favorites', '/favorites' %></li>
  <li><a href="#">My Pictures</a></li>
  <li><a href="#">My Messages</a></li>
  <li> <%= link_to "Upload", new_picture_path %> </li>
