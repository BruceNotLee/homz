    <%= link_to image_tag("icon.png",size: "36x36"), feed_path, class:'brand-logo', id:'location' %>
  </div>
  <div class="center" id="zip_search">
    <%= form_tag feed_path do %>
    <%= text_field_tag :zip, nil, placeholder: "Search homz by zip code" %>
